{
  "version": 2,
  "alias": [
    "qcbrunch.com",
    "www.qcbrunch.com"
  ],
  "builds": [
    {
      "src": "*.html",
      "use": "@now/static"
    },
    {
      "src": "*/*.html",
      "use": "@now/static"
    },
    {
      "src": "css/*.css",
      "use": "@now/static"
    },
    {
      "src": "images/*",
      "use": "@now/static"
    },
    {
      "src": "js/*.js",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/stats",
      "dest": "/stats.html"
    }
  ],
  "regions": [
    "all"
  ],
  "public": false,
  "github": {
    "enabled": false
  }
}
